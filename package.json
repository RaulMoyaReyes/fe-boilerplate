{
  "name": "fe-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for frontend porject",
  "main": "dist/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "cd dist && http-server",
    "clean": "rm -rf dist && mkdir dist",
    "assets": "cp index.html dist/index.html",
    "build": "npm run clean && npm run assets && webpack --progress --colors",
    "watch": "npm run clean && npm run assets && webpack --progress --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaulMoyaReyes/fe-boilerplate.git"
  },
  "author": "Ra√∫l Moya Reyes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RaulMoyaReyes/fe-boilerplate/issues"
  },
  "homepage": "https://github.com/RaulMoyaReyes/fe-boilerplate#readme",
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-preset-latest": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "webpack": "1.14.0"
  }
}
