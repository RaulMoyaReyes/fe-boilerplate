  },
  "homepage": "https://github.com/raulmoyareyes/fe-boilerplate",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.3.3",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.28.0",
    "jasmine": "^2.5.3",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss": "^5.2.16",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.js",
    "clean": "rimraf -r ./dist",
    "build": "npm run clean && webpack $B_PROD $B_WATCH --config config/webpack.config.js",
    "build:dev": "npm run build",
    "build:prod": "B_PROD=-p npm run build",
    "watch": "B_WATCH=-w npm run build",
    "watch:dev": "npm run watch",
    "watch:prod": "B_PROD=-p npm run watch",
    "test": "karma start config/karma.conf.js"
  },
  "config": {
    "entryFile": "index",
    "outputFile": "bundle",
    "buildFolder": "dist"
  }
}
